{
  "version": 2,
  "builds": [
    {
      "src": "card_api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/divination",
      "dest": "card_api/main.py"
    },
    {
      "src": "/manifest.json",
      "headers": { "Cache-Control": "public, max-age=0, must-revalidate" },
      "dest": "public/manifest.json"
    },
    {
      "src": "/sw.js",
      "headers": { "Cache-Control": "public, max-age=0, must-revalidate" },
      "dest": "public/sw.js"
    },
    {
      "src": "/icons/(.*)",
      "dest": "public/icons/$1"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}